<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" hgap="5.0" vgap="5.0" xmlns="http://javafx.com/javafx/10.0.2-internal">
    <stylesheets>
        <URL value="@/css/forme.css"/>
    </stylesheets>
    <columnConstraints>
        <ColumnConstraints/>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints minHeight="10.0" prefHeight="30.0"/>
        <RowConstraints/>
        <RowConstraints/>
        <RowConstraints/>
    </rowConstraints>
    <opaqueInsets>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </opaqueInsets>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <Label text="Naziv:"/>
    <TextField fx:id="fieldNaziv" GridPane.columnIndex="1"/>
    <Label text="Broj stanovnika:" GridPane.rowIndex="1"/>
    <TextField fx:id="fieldBrojStanovnika" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    <Label layoutX="20.0" layoutY="56.0" text="Poštanski broj:" GridPane.rowIndex="2"/>
    <TextField fx:id="fieldPostanskiBroj" layoutX="131.0" layoutY="51.0" GridPane.columnIndex="1"
               GridPane.rowIndex="2"/>
    <Label text="Država:" GridPane.rowIndex="3"/>
    <ChoiceBox fx:id="choiceDrzava" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
    <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="4">
        <buttons>
            <Button fx:id="btnOk" mnemonicParsing="false" onAction="#clickOk" text="Ok"/>
            <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#clickCancel" text="Cancel"/>
        </buttons>
    </ButtonBar>
</GridPane>
